{
  "name": "@alova/shared",
  "version": "0.0.1",
  "type": "module",
  "author": "Scott Hu",
  "module": "dist/alova.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "lint": "eslint --ext .ts,.js src/**",
    "lint:fix": "eslint --ext .ts,.js src/** --fix",
    "build": "tsc --project tsconfig.build.json",
    "coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "commit": "git add . && git-cz && git push"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alovajs/alova.git"
  },
  "bugs": {
    "url": "https://github.com/alovajs/alova/issues"
  },
  "exports": {
    "./assert": "./dist/assert.js",
    "./function": "./dist/function.js",
    "./vars": "./dist/vars.js",
    "./types": "./dist/types.js",
    "./event": "./dist/event.js",
    "./createEventManager": "./dist/createEventManager.js"
  },
  "typesVersions": {
    "*": {
      "assert": [
        "./dist/assert.d.ts"
      ],
      "function": [
        "./dist/function.d.ts"
      ],
      "vars": [
        "./dist/vars.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "event": [
        "./dist/event.d.ts"
      ],
      "createEventManager": [
        "./dist/createEventManager.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@alova/scripts": "workspace:*"
  }
}

{
  "core": {
    "packageName": "alova",
    "input": "src/index.ts",
    "output": "dist/alova.{suffix}.{ext}",
    "external": {
      "@alova/shared/function": null,
      "@alova/shared/vars": null,
      "@alova/shared/assert": null
    }
  },
  "fetch": {
    "packageName": "alovaFetch",
    "input": "src/predefine/adapterFetch.ts",
    "external": {
      "@alova/shared/function": null,
      "@alova/shared/vars": null,
      "@alova/shared/assert": null
    },
    "output": "./dist/adapter/fetch.{suffix}.{ext}"
  }
}
